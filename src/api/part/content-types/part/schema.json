{
  "kind": "collectionType",
  "collectionName": "parts",
  "info": {
    "singularName": "part",
    "pluralName": "parts",
    "displayName": "Part",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "minLength": 1,
      "unique": true
    },
    "number": {
      "type": "text",
      "required": true,
      "minLength": 1
    },
    "material": {
      "type": "text",
      "required": true,
      "minLength": 1
    },
    "weight": {
      "type": "string",
      "required": true,
      "minLength": 1
    },
    "media": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "richtext",
      "required": true,
      "minLength": 1
    },
    "active": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "featured": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "supplier": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::supplier.supplier"
    }
  }
}
